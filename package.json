{
  "name": "@astro-api/n8n-nodes-astrology",
  "version": "0.1.0",
  "description": "n8n community node for Astrology API - professional astrological calculations, charts, horoscopes, and interpretations",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp nodes/Astrology/*.svg dist/nodes/Astrology/",
    "lint": "eslint \"{nodes,credentials}/**/*.ts\"",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "astrology",
    "astrology-api",
    "horoscope",
    "natal-chart",
    "birth-chart",
    "zodiac",
    "planetary-positions",
    "synastry",
    "transit",
    "vedic-astrology",
    "chinese-astrology",
    "bazi",
    "swiss-ephemeris"
  ],
  "author": "Astrology API <support@astrology-api.io>",
  "homepage": "https://astrology-api.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/astro-api/n8n-nodes-astrology"
  },
  "bugs": {
    "url": "https://github.com/astro-api/n8n-nodes-astrology/issues"
  },
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "commonjs",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AstrologyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Astrology/Astrology.node.js"
    ]
  },
  "peerDependencies": {
    "n8n-workflow": "1.55.0"
  },
  "devDependencies": {
    "@types/node": "20.17.3",
    "eslint": "9.15.0",
    "n8n-workflow": "1.55.0",
    "prettier": "3.4.1",
    "typescript": "5.5.4",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.16.0"
  }
}
